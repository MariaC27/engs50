#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <stdbool.h>

int main(void){
	while (true){
		char* input;
		printf(">");
		scanf("%c", input);
		printf("%c\n", input);
	}	
	return 0;
}
